 #include "include/move.h"

void turn(int direcao, int graus){
  if (direcao == ESQUERDA) {
    turnLeftPID(graus);
  } else if (direcao == DIREITA) {
    turnRightPID(graus);
  }
}

task foward(){
  start fowardPID;
}

