#include "include/ramp.h"

int isRamp(int turnVec[][]) {
  for(int i = 0; i < 3; i++) {
    for(int j = 0; j < 3; j ++) {
      if (turnVec[i][j] == 0) {
        return FALSE;
      }
    }
  }
  return TRUE;
}

void foundRamp() {
  PlayTone(500, 500);
  Wait(1000);
  PlayTone(500, 500);
  Wait(1000);
  PlayTone(500, 500);
}
